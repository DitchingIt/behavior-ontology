property_value: http://purl.org/dc/elements/1.1/date 2011-04-14T01:20:02Z xsd:string
property_value: IAO_0000115 "Undirected behavior involving exercise and coordination, and often associated with repetition and 'practice' of adult behavior patterns in a non-functional context  [NBO:AC]" xsd:string
property_value: namespace "behavior_ontology" xsd:string

